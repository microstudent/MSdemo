<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.microstudent.app.msdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/avatar"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ColorfulAnimView.ui.ColorAnimActivity" />
        <activity android:name=".DeletableEditText.ui.EditTextActivity" />
        <activity android:name=".IndicatorView.ui.WelcomeActivity" />
        <activity android:name=".CustomizeRecyclerView.ui.RecyclerViewActivity" />
        <activity android:name=".PullToZoomScrollView.ui.PullToZoomActivity" />
    </application>

</manifest>